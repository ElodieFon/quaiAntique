{% extends 'base.html.twig' %}

{% block title %}Le Quai Antique / contact{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('css/contact.css') }}" type="text/css" rel="stylesheet">
{% endblock %}

{% block body %}

<main>
    {% include "base/header.html.twig" %}
    <div class="bg-success text-center text-light mt-2">
        {{message}}
    </div>
    <div class="d-flex justify-content-evenly mt-5 mb-5">
        <div class=" col-5  bg-light  p-3 ">
            {{ form_start(form) }}
            {{ form_rest(form) }}
        </div>
        <div class=" d-flex align-items-center justify-content-center col-5">
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2793.296758608161!2d5.922983099999998!3d45.56447059999999!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x478ba8fc1ffc8713%3A0x9e950d0009d18de!106 rue croix d&#39;Or%2C%2073000%20Chamb%C3%A9ry!5e0!3m2!1sfr!2sfr!4v1677493304435!5m2!1sfr!2sfr" 
                width="400" 
                height="400" 
                style="border:0;" 
                allowfullscreen="" 
                loading="lazy" 
                referrerpolicy="no-referrer-when-downgrade">
            </iframe>
        </div>
    </div>
</main>
{% endblock %}
